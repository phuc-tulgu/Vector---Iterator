#include<iostream>
#include<vector>

using namespace std;

void in_put(vector<int> &v, int n){
	for(int i = 0; i < n; i++){
		cout << "Value elements ["<<i<<"] :";
		int index;
		cin >> index;
		v.push_back(index);
	}
}

void out_put(vector<int> v, int n){
	for(int i = 0; i < v.size(); i++){
		cout << " " << v.at(i);
	}
}

int main(){
	int n;
	cout << "Enter n vector to create:";
	cin >> n;
	vector <int> v;
	in_put(v,n);
	out_put(v,n);
	
} 
